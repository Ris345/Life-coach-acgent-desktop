{
  "manifest_version": 3,
  "name": "LifeOS Tab Tracker",
  "version": "1.0.0",
  "description": "Tracks active browser tabs for LifeOS desktop app",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://127.0.0.1:14200/*",
    "http://localhost:14200/*"
  ],
}

